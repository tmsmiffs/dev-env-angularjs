{
    "root": true,
    "extends": [
        "plugin:angular/johnpapa",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-use-before-define": 0,
        "no-console": 1
    }
}
