  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.1",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "5.0.0",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-watch": "3.1.0",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.11.0",
    "json-schema-faker": "0.4.0",
    "json-server": "0.10.1",
    "localtunnel": "1.8.2",
    "mocha": "3.4.2",
    "nock": "9.0.13",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.1",
    "style-loader": "0.18.1",
    "surge": "0.19.0",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
